{
  "serviceName": "${SERVICENAME}",
  "httpServer": {
    "port": "${HTTPSERVER_PORT}",
    "env": "${HTTPSERVER_ENV}",
    "basePath": "${HTTPSERVER_BASEPATH}",
    "timeout": "${HTTPSERVER_TIMEOUT}",
    "host": "${HTTPSERVER_HOST}"
  },
  "logger": {
    "level": "debug",
    "format": "console"
  },
  "database": {
    "password": "${DATABASE_PASSWORD}",
    "user": "${DATABASE_USER}",
    "url": "${DATABASE_URL}",
    "schema": "${DATABASE_SCHEMA}",
    "db": "postgres"
  },
  "kafka": {
    "addr": [
      "${KAFKA_ADDR}"
    ],
    "producers": [
      {
        "topic": "",
        "async": false,
        "batchSize": 10,
        "batchTimeout": 100
      }
    ],
    "consume": {
      "consumers": [
        {
          "topic": ""
        }
      ]
    }
  },
  "inbox": {
    "put": {},
    "poll": {}
  }
}